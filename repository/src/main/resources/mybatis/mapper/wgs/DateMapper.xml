<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wgs.mapper.utils.DateMapper">

    <resultMap id="DateMap" type="com.wgs.entity.utils.DateBean">

        <result column="now" property="now"/>
    </resultMap>

    <!-- 获取当前时间 -->
    <select id="getNow" resultMap="java.lang.Object">
        SELECT NOW() as now from DUAL
    </select>


</mapper>